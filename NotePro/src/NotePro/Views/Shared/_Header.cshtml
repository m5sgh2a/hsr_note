<meta charset="utf-8" />
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
<title>@ViewData["Title"] - NotePro</title>

<environment names="Development">
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootswatch/3.3.6/spacelab/bootstrap.min.css" title="main" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/css/star-rating.css" />
</environment>
<environment names="Staging,Production">
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootswatch/3.3.6/spacelab/bootstrap.min.css" title="main"
          asp-fallback-href="~/lib/bootstrap/dist/css/bootstrap.min.css"
          asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/star-rating.css" asp-append-version="true" />
</environment>

@Html.ApplicationInsightsJavaScript(TelemetryConfiguration)